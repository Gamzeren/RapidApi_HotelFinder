
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zxx">

@await Component.InvokeAsync("_UIHeadComponentPartial")

<body>
    <div id="preloder">
        <div class="loader"></div>
    </div>

    @await Component.InvokeAsync("_UIHeaderComponentPartial")

    @await Component.InvokeAsync("_UIBookingComponentPartial")

    @await Component.InvokeAsync("_UIAboutComponentPartial")

    @await Component.InvokeAsync("_UIServicesComponentPartial")

    @await Component.InvokeAsync("_UITestimonialComponentPartial")

    @await Component.InvokeAsync("_UITourComponentPartial")

    @await Component.InvokeAsync("_UIFooterComponentPartial")
    
    <!-- Search model Begin -->
    <div class="search-model">
        <div class="h-100 d-flex align-items-center justify-content-center">
            <div class="search-close-switch"><i class="icon_close"></i></div>
            <form class="search-model-form">
                <input type="text" id="search-input" placeholder="Search here.....">
            </form>
        </div>
    </div>
    <!-- Search model end -->
    @await Component.InvokeAsync("_UIScriptComponentPartial")
</body>
</html>